# Supabase Configuration
# You can find these keys in your Supabase project settings.
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url-here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key-here

# Stripe Configuration
# You can find these keys in your Stripe dashboard.
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# AI Services for Deep Research
# Create a .env.local file and add your API keys there.
OPENAI_API_KEY="your-openai-api-key-here"
GEMINI_API_KEY="your-gemini-api-key-here"

# Logging Configuration
LOG_LEVEL=info  # debug, info, warn, error

# Sentry Configuration (for error monitoring)
# Get this from your Sentry project settings
NEXT_PUBLIC_SENTRY_DSN=https://your_key@o123456.ingest.sentry.io/123456

# Optional: Only needed for automated source maps upload
SENTRY_ORG=your_sentry_org_slug
SENTRY_PROJECT=your_sentry_project_slug